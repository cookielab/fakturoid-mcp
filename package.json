{
	"name": "mcp-server",
	"version": "0.1.0",
	"description": "MCP Server written in TypeScript",
	"type": "module",
	"main": "dist/main.js",
	"bin": "dist/main.js",
	"scripts": {
		"build": "tsc --noEmit false",
		"dev": "tsx watch src/main.ts",
		"types": "tsc",
		"lint": "biome lint src/",
		"format": "biome format --write --fix src/"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.11.0",
		"@types/express": "5.0.3",
		"dotenv": "16.5.0",
		"express": "5.1.0",
		"node-fetch": "3.3.2",
		"pino": "9.7.0",
		"pino-pretty": "13.0.0",
		"zod": "3.25.64"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.0-beta.6",
		"@types/node": "22.15.31",
		"tsx": "4.20.2",
		"typescript": "5.8.3"
	},
	"engines": {
		"node": "23.11.0"
	}
}
