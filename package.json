{
	"name": "fakturoid-mcp-server",
	"version": "0.1.0",
	"description": "MCP Server written in TypeScript for Fakturoid",
	"type": "module",
	"main": "dist/main.js",
	"bin": "dist/main.js",
	"files": ["dist"],
	"exports": {
		".": "./src/server.ts",
		"./strategy": "./src/auth/strategy.ts"
	},
	"scripts": {
		"build": "tsc --noEmit false",
		"dev": "NODE_ENV=development tsx --watch src/main.ts",
		"types": "tsc",
		"lint": "biome lint src/",
		"format": "biome format --write --fix src/",
		"ui": "pnpx @modelcontextprotocol/inspector",
		"test": "vitest",
		"cf:generate": "wrangler types",
		"cf:dev": "wrangler dev",
		"cf:dev:debug": "wrangler dev --inspect",
		"cf:deploy": "wrangler deploy"
	},
	"dependencies": {
		"@cloudflare/workers-oauth-provider": "0.0.5",
		"@modelcontextprotocol/sdk": "1.16.0",
		"agents": "0.0.109",
		"dotenv": "17.2.0",
		"express": "5.1.0",
		"hono": "4.8.5",
		"node-fetch": "3.3.2",
		"pino": "9.7.0",
		"pino-pretty": "13.0.0",
		"zod": "4.0.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@cookielab.io/biome-config": "0.5.0",
		"@modelcontextprotocol/inspector": "0.16.1",
		"@snaplet/copycat": "6.0.0",
		"@types/express": "5.0.3",
		"@types/node": "24.1.0",
		"@vitest/coverage-v8": "3.2.4",
		"tailwindcss": "4.1.11",
		"tsx": "4.20.3",
		"typescript": "5.8.3",
		"vitest": "3.2.4",
		"wrangler": "4.25.1"
	},
	"engines": {
		"node": "24.2.0"
	},
	"packageManager": "pnpm@10.13.1"
}
