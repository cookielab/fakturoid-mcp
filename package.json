{
	"name": "fakturoid-mcp-server",
	"version": "0.1.0",
	"description": "MCP Server written in TypeScript for Fakturoid",
	"type": "module",
	"main": "dist/main.js",
	"bin": "dist/main.js",
	"exports": {
		".": "./src/server.ts",
		"./strategy": "./src/auth/strategy.ts"
	},
	"scripts": {
		"build": "tsc --noEmit false",
		"dev": "tsx --watch src/main.ts",
		"types": "tsc",
		"lint": "biome lint src/",
		"format": "biome format --write --fix src/",
		"ui": "pnpx @modelcontextprotocol/inspector"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.15.0",
		"dotenv": "17.2.0",
		"express": "5.1.0",
		"node-fetch": "3.3.2",
		"pino": "9.7.0",
		"pino-pretty": "13.0.0",
		"zod": "4.0.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"@cookielab.io/biome-config": "0.3.1",
		"@modelcontextprotocol/inspector": "0.16.0",
		"@types/express": "5.0.3",
		"@types/node": "24.0.12",
		"tailwindcss": "4.1.11",
		"tsx": "4.20.3",
		"typescript": "5.8.3"
	},
	"engines": {
		"node": "24.2.0"
	},
	"packageManager": "pnpm@10.12.1"
}
